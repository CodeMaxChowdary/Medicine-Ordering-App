<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StaffActivity">
    <Button
        android:id="@+id/staff_bt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/Tab_Name"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/buttonshape"
        android:text="@string/add_heart_care_medicines"/>
    <Button
        android:id="@+id/staff_bt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/Tab_Name"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:layout_below="@id/staff_bt1"
        android:layout_centerHorizontal="true"
        android:background="@drawable/buttonshape"
        android:text="@string/add_kidney_care_medicines"/>
    <Button
        android:id="@+id/staff_bt3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/staff_bt2"
        android:textColor="@color/Tab_Name"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/buttonshape"
        android:text="@string/add_lung_care_medicines"/>
    <Button
    android:id="@+id/staff_bt4"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="@color/Tab_Name"
    android:layout_below="@id/staff_bt3"
    android:textStyle="bold"
        android:layout_marginTop="10dp"
    android:layout_centerHorizontal="true"
    android:background="@drawable/buttonshape"
    android:text="@string/add_mental_care_medicines"/>
    <Button
        android:id="@+id/staff_bt5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/Tab_Name"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/staff_bt4"
        android:background="@drawable/buttonshape"
        android:text="@string/add_stomach_care_medicines"/>
    <Button
        android:id="@+id/staff_bt6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/Tab_Name"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:layout_below="@id/staff_bt5"
        android:background="@drawable/buttonshape"
        android:text="@string/add_joint_care_medicines"/>
    <Button
        android:id="@+id/staff_bt7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/Tab_Name"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/buttonshape"
        android:layout_below="@id/staff_bt6"
        android:text="@string/add_liver_care_medicines"/>
    <Button
        android:id="@+id/staff_bt8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/Tab_Name"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/buttonshape"
        android:layout_below="@id/staff_bt7"
        android:text="@string/orders"/>



</RelativeLayout>